<p data-bind="if: currentCountry().editable">
    <a href="#" class="mb-5" data-bind="click: function() {regionMode('edit')}">Edit</a>
</p>
<p data-bind="if: !currentCountry().editable">
    <a href="#" class="mb-5" data-toggle="modal"
       data-target="#new-country" data-bind="text: mode() == 'scenario' ? 'Edit' : 'Create editable copy'"></a>
</p>
<div class="text-secondary">
    <div class="d-inline mr-3 text-secondary">
        <strong>Year</strong>: 2013
    </div>
    <div class="d-inline mr-3">
        <strong>Age range</strong>: 0 - 5
    </div>
    <div class="d-inline mr-3">
        <strong>Model estimated baseline under 5 prevalence</strong>:42.6
    </div>
</div>

<div class="mt-5" style="width: 700px;">
    <canvas class="seasonal"></canvas>
</div>
<div style="width: 400px; height: 400px" class="mt-5">
    <canvas class="vectors"></canvas>
</div>